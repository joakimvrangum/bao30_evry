{
	"name": "test_potato",
	"properties": {
		"description": "loading customer data ",
		"activities": [
			{
				"name": "Copy Data1",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "RelationalSource",
						"query": "select * from `bank_customer`"
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"CustomerId": "id",
							"Geography": "geography"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "RelationalTable1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "OutputSqlDataset",
						"type": "DatasetReference"
					}
				]
			}
		]
	}
}